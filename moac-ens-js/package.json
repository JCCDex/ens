{
  "name": "moac-ens",
  "version": "0.1.1",
  "description": "moac ens javascript sdk",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "deploy": "./publish.sh",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "build": "tsc",
    "test": "npm run test:node && npm run test:browser",
    "test:node": "npm run build && nyc --extension=.ts --reporter=lcov --reporter=html --reporter=text mocha --timeout=3000 test/*.spec.js",
    "test:browser": "npm run build && karma start karma.conf.js",
    "tslint": "tslint --project tsconfig.json --config tslint.json",
    "pretty": "prettier --write {src,test}/**/*.{js,ts}"
  },
  "keywords": [
    "moac",
    "ens"
  ],
  "devDependencies": {
    "browserify": "^16.5.0",
    "chai": "^4.2.0",
    "coveralls": "^3.0.9",
    "husky": "^4.0.5",
    "karma": "^4.4.1",
    "karma-browserify": "^6.1.0",
    "karma-chrome-launcher": "^3.1.0",
    "karma-detect-browsers": "^2.3.3",
    "karma-env-preprocessor": "^0.1.1",
    "karma-firefox-launcher": "^1.2.0",
    "karma-mocha": "^1.3.0",
    "karma-webpack": "^4.0.2",
    "mocha": "^10.2.0",
    "nyc": "^15.0.0",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "sinon": "^8.0.4",
    "ts-loader": "^6.2.1",
    "tslint": "^5.20.1",
    "typescript": "^3.7.3",
    "webpack": "^4.41.2"
  },
  "dependencies": {
    "jcc-moac-utils": "^0.2.7"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "author": "GinMu",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JCCDex/ens.git"
  },
  "bugs": {
    "url": "https://github.com/JCCDex/ens/issues"
  },
  "homepage": "https://github.com/JCCDex/ens"
}
